<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Title -->
    <title>虹账付</title>

    <!-- Required Meta Tags Always Come First -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">

    <!-- Favicon -->
    <link rel="shortcut icon" href="/img/logo/logo.png">

    <!-- CSS Global Compulsory -->
    <link rel="stylesheet" href="/vendor/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="/vendor/icon-line/css/simple-line-icons.css">

    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="/vendor/icon-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/vendor/icon-line-pro/style.css">
    <link rel="stylesheet" href="/vendor/icon-hs/style.css">
    <link rel="stylesheet" href="/vendor/animate.css">
    <link rel="stylesheet" href="/vendor/hamburgers/hamburgers.min.css">
    <link rel="stylesheet" href="/vendor/hs-megamenu/src/hs.megamenu.css">
    <link rel="stylesheet" href="/vendor/malihu-scrollbar/jquery.mCustomScrollbar.min.css">

    <!-- CSS Unify Theme -->
    <link rel="stylesheet" href="/css/styles.e-commerce.css">
    <link rel="stylesheet" href="/css/style.css">

    <!-- CSS Customization -->
    <link rel="stylesheet" href="/css/custom.css">
    <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Roboto%3A300%2C400%2C500%2C700%7CPlayfair+Display%7CRaleway%7CSpectral%7CRubik">

    <link rel="stylesheet" href="/vendor/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="/vendor/icon-line/css/simple-line-icons.css">

    <!-- CSS Implementing Plugins -->
    <link rel="stylesheet" href="/vendor/icon-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/vendor/icon-line-pro/style.css">
    <link rel="stylesheet" href="/vendor/icon-hs/style.css">
    <link rel="stylesheet" href="/vendor/animate.css">
    <link rel="stylesheet" href="/vendor/hamburgers/hamburgers.min.css">
    <link rel="stylesheet" href="/vendor/hs-megamenu/src/hs.megamenu.css">
    <link rel="stylesheet" href="/vendor/malihu-scrollbar/jquery.mCustomScrollbar.min.css">

    <!-- CSS Unify Theme -->
    <link rel="stylesheet" href="/css/styles.e-commerce.css">

    <!-- CSS Customization -->
    <link rel="stylesheet" href="/css/custom.css">


    <!-- CSS Global Compulsory -->
    <script src="/vendor/jquery/jquery.min.js"></script>
    <!--<script src="/assets/vendor/jquery-migrate/jquery-migrate.min.js"></script>-->
    <script src="/vendor/popper.min.js"></script>
    <script src="/vendor/bootstrap/bootstrap.min.js"></script>

    <!-- JS Implementing Plugins -->
    <script src="/vendor/hs-megamenu/src/hs.megamenu.js"></script>
    <script src="/vendor/malihu-scrollbar/jquery.mCustomScrollbar.concat.min.js"></script>

    <!-- JS Unify -->
    <script src="/js/hs.core.js"></script>
    <script src="/js/components/hs.header.js"></script>
    <script src="/js/helpers/hs.hamburgers.js"></script>
    <script src="/js/components/hs.dropdown.js"></script>
    <script src="/js/components/hs.scrollbar.js"></script>
    <script src="/js/components/hs.go-to.js"></script>

    <!-- JS Customization -->
    <script src="/js/custom.js"></script>
</head>

<body>
<main>
    <!-- Header -->
    <header id="js-header" class="u-header u-header--static u-shadow-v19">
        <!-- Top Bar -->
        <div class="u-header__section g-brd-bottom g-brd-gray-light-v4 g-bg-black g-transition-0_3">
            <div class="container">
                <div class="row justify-content-between align-items-center g-mx-0--lg">
                    <div class="col-auto g-hidden-xs-down g-color-white-opacity-0_6 g-font-weight-400 g-pl-15 g-pl-0--sm g-py-14">
                        <i class="icon-communication-163 u-line-icon-pro g-font-size-18 g-valign-middle g-color-white-opacity-0_8 g-mr-10 g-mt-minus-2"></i>
                        服务热线：800 1234 4321
                    </div>

                    <div class="col-auto g-pr-15 g-pr-0--sm">
                        <!-- List -->
                        <ul class="list-inline g-overflow-hidden g-pt-1 g-mx-minus-4 mb-0">
                            <li class="list-inline-item g-mx-4">
                                <a class="g-color-white-opacity-0_6 g-color-primary--hover g-font-weight-400 g-text-underline--none--hover" href="#">平台网站</a>
                            </li>

                            <li class="list-inline-item g-color-white-opacity-0_3 g-mx-4">|</li>
                            <li class="list-inline-item g-mx-4">
                                <a class="g-color-white-opacity-0_6 g-color-primary--hover g-font-weight-400 g-text-underline--none--hover" href="#">个人门户</a>
                            </li>

                            <li class="list-inline-item g-color-white-opacity-0_3 g-mx-4">|</li>
                            <!-- Account -->
                            <li class="list-inline-item g-mx-4">
                                <a class="g-color-white-opacity-0_6 g-color-primary--hover g-font-weight-400 g-text-underline--none--hover" href="#">商户门户</a>
                            </li>
                            <!-- End Account -->
                        </ul>
                        <!-- End List -->
                    </div>
                </div>
            </div>
        </div>
        <!-- End Top Bar -->

        <div class="u-header__section u-header__section--light g-bg-white g-transition-0_3 g-py-10">
            <nav class="js-mega-menu navbar navbar-expand-lg">
                <div class="container">
                    <!-- Responsive Toggle Button -->
                    <button class="navbar-toggler navbar-toggler-right btn g-line-height-1 g-brd-none g-pa-0 g-pos-abs g-top-3 g-right-0" type="button"
                            aria-label="Toggle navigation"
                            aria-expanded="false"
                            aria-controls="navBar"
                            data-toggle="collapse"
                            data-target="#navBar">
                <span class="hamburger hamburger--slider g-pr-0">
                  <span class="hamburger-box">
                    <span class="hamburger-inner"></span>
                  </span>
                </span>
                    </button>
                    <!-- End Responsive Toggle Button -->

                    <!-- Logo -->
                    <a class="">
                        <img src="/img/logo/logo.png" width="20%" height="20%">
                        <span class="h4">虹账付</span>
                    </a>
                    <!-- End Logo -->
                </div>
            </nav>
        </div>

    </header>
    <!-- End Header -->

    <!-- Signup -->
    <div class="container g-mt-60">
        <!-- Static Process -->
        <div class="row">
            <div class="col-sm-6 col-lg-3 g-mb-60">
                <!-- Static Process -->
                <div class="text-center">
                    <h3 class="h5 g-font-weight-400 g-mb-10 g-color-gray-dark-v4">创建账户</h3>
                    <p class="mb-0">${processOneAdvice}</p>
                    <i class="u-dot-line-v1-2 g-brd-transparent--before g-brd-gray-light-v2--after g-mt-20">
                        <#if oneFlag == 1><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2 g-bg-primary" [ngClass]="{'g-bg-primary' : oneFlag }"></span>
                        <#else><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2" [ngClass]="{'g-bg-primary' : oneFlag }"></span></#if>
                    </i>
                </div>
                <!-- End Static Process -->
            </div>

            <div class="col-sm-6 col-lg-3 g-mb-60">
                <!-- Static Process -->
                <div class="text-center">
                    <h3 class="h5 g-font-weight-400 g-mb-10 g-color-gray-dark-v4">完善信息</h3>
                    <p class="mb-0">${processTwoAdvice}</p>
                    <i class="u-dot-line-v1-2 g-brd-gray-light-v2--before g-brd-gray-light-v2--after g-mt-20">
                        <#if twoFlag == 1><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2 g-bg-primary" [ngClass]="{'g-bg-primary' : twoFlag }"></span>
                        <#else ><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2" [ngClass]="{'g-bg-primary' : twoFlag }"></span></#if>
                    </i>
                </div>
                <!-- End Static Process -->
            </div>

            <div class="col-sm-6 col-lg-3 g-mb-60">
                <!-- Static Process -->
                <div class="text-center">
                    <h3 class="h5 g-font-weight-400 g-mb-10 g-color-gray-dark-v4">银行卡</h3>
                    <p class="mb-0">${processThreeAdvice}</p>
                    <i class="u-dot-line-v1-2 g-brd-gray-light-v2--before g-brd-gray-light-v2--after g-mt-20">
                        <#if threeFlag == 1><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2 g-bg-primary" [ngClass]="{'g-bg-primary' : threeFlag }"></span>
                        <#else ><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2" [ngClass]="{'g-bg-primary' : threeFlag }"></span></#if>
                    </i>
                </div>
                <!-- End Static Process -->
            </div>

            <div class="col-sm-6 col-lg-3 g-mb-60">
                <!-- Static Process -->
                <div class="text-center">
                    <h3 class="h5 g-font-weight-400 g-mb-10 g-color-gray-dark-v4">注册完成</h3>
                    <p class="mb-0">恭喜您，完成申请，请等待通知！</p>
                    <i class="u-dot-line-v1-2 g-brd-gray-light-v2--before g-brd-transparent--after g-mt-20">
                        <#if fourFlag == 1><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2 g-bg-primary" [ngClass]="{'g-bg-primary' : fourFlag }"></span>
                        <#else ><span class="u-dot-line-v1__inner g-bg-primary--before g-brd-gray-light-v2" [ngClass]="{'g-bg-primary' : fourFlag }"></span></#if>
                    </i>
                </div>
                <!-- End Static Process -->
            </div>
        </div>
        <!-- End Static Process -->
    </div>

    <div class="container g-mt-60 g-mb-60">
        <div id="shortcode2">
            <div class="shortcode-html">
                <!-- Nav tabs -->
                <ul class="nav justify-content-center u-nav-v2-1 u-nav-rounded-3 g-mb-50" role="tablist" data-target="heading-01" data-tabs-mobile-type="slide-up-down" data-btn-classes="btn btn-md btn-block u-btn-outline-primary">
                    <li class="nav-item" *ngIf="perdisFlag">
                        <#if perdisFlag == 1><a class="nav-link"  data-toggle="tab" href="#heading-01-1" role="tab" (click)="person()" [ngClass]="{'active':peractiveFlag}">我是个人</a></#if>
                    </li>
                    <li class="nav-item" *ngIf="comdisFlag">
                        <#if comdisFlag == 1><a class="nav-link"   data-toggle="tab" href="#heading-01-2" role="tab" (click)="company()" [ngClass]="{'active':comactiveFlag}">我是企业</a></#if>
                    </li>
                </ul>
                <!-- End Nav tabs -->
                <!-- Tab panes -->
                <div id="nav-1-2-primary-hor-center" class="tab-content">
                    <div class="tab-pane fade show active" id="heading-01-1" role="tabpanel">
                    <#--${alertErr}-->
                        <#if flag == 2 >
                            <div class="alert alert-dismissible fade show g-bg-red g-color-white rounded-0 g-width-40x--md mx-auto" role="alert" *ngIf="alertErr">
                                <button type="button" class="close u-alert-close--light" data-dismiss="alert" aria-label="Close">
                                    <span aria-hidden="true" >&times;</span>
                                </button>

                                <div class="media">
                                  <span class="d-flex g-mr-10 g-mt-5">
                                    <i class="icon-ban g-font-size-25"></i>
                                  </span>
                                    <span class="media-body align-self-center">
                                    <strong></strong>{{returnInfo}}
                                  </span>
                                </div>
                            </div>
                        </#if>

                        <#if personRegisterFlag == 1 >
                            <form class="" name="personRegisterForm"  *ngIf="personRegisterFlag" action="/perOpen">
                            <div class="text-left mx-auto g-width-40x--md">
                                <!--<div class="form-group row g-mb-25 text-center">-->
                                <!--<label class="col-sm-4 col-form-label g-mt-10 g-color-gray-dark-v4"></label>-->
                                <!--<div class="red text-center" style="color: red"> <h6>{{returnInfo}}</h6></div>-->

                                <!--</div>-->
                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">手机号码</label>

                                    <div class="col-sm-9">
                                        <input class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" type="text" placeholder="输入手机号" formControlName="mobile">
                                        <#--<div *ngIf="personRegisterForm.controls['mobile'].hasError('required') && personRegisterForm.controls['mobile'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 手机号不能为空！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personRegisterForm.controls['mobile'].hasError('minlength') || personRegisterForm.controls['mobile'].hasError('maxlength') && personRegisterForm.controls['mobile'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 手机号为11位！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personRegisterForm.controls['mobile'].hasError('validateTel') && personRegisterForm.controls['mobile'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 手机号只能由数字组成！</i>-->
                                        <#--</div>-->
                                        <!--<div *ngIf="personRegisterForm.controls['mobile'].hasError('notValid') && personRegisterForm.controls['mobile'].touched" class="text-warning bigger-110 orange">-->
                                        <!--<i class="ace-icon fa fa-exclamation-triangle"> {{returnInfo}}</i>-->
                                        <!--</div>-->
                                    </div>
                                </div>

                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">手机验证码</label>

                                    <div class="col-sm-6">
                                        <input class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" type="text" placeholder="输入短信验证码" formControlName="captcha">
                                        <#--<div *ngIf="personRegisterForm.controls['captcha'].hasError('required') && personRegisterForm.controls['captcha'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 短信校验码不能为空！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personRegisterForm.controls['captcha'].hasError('notValid') && personRegisterForm.controls['captcha'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> {{returnInfo}}</i>-->
                                        <#--</div>-->
                                    </div>

                                    <div class="text-center">
                                        <button class="btn u-btn-primary g-font-size-12 text-uppercase g-py-12 g-px-25" type="submit" (click)="sms($event, this.personRegisterForm.controls['mobile'].value)" [disabled]="this.personRegisterForm.controls['mobile'].invalid">发送验证码</button>
                                    </div>
                                </div>

                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">登录密码</label>

                                    <div class="col-sm-9">
                                        <input class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" type="password" placeholder="输入登录密码" formControlName="password">
                                        <#--<div *ngIf="personRegisterForm.controls['password'].hasError('required') && personRegisterForm.controls['password'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 密码不能为空！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personRegisterForm.controls['password'].hasError('validateNull') && personRegisterForm.controls['password'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 密码不能为空格！</i>-->
                                        <#--</div>-->
                                    </div>
                                </div>

                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">确认密码</label>

                                    <div class="col-sm-9">
                                        <input class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" type="password" placeholder="输入确认密码" formControlName="perPassword" (change)="equalCheck()">
                                        <#--<div *ngIf="personRegisterForm.controls['perPassword'].hasError('required') && personRegisterForm.controls['perPassword'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 确认密码不一致！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personRegisterForm.controls['perPassword'].hasError('notValid') && personRegisterForm.controls['perPassword'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 确认密码不一致！</i>-->
                                        <#--</div>-->
                                    </div>
                                </div>

                                <div class="text-center">
                                    <button class="btn u-btn-primary g-font-size-12 text-uppercase g-py-12 g-px-25" type="submit">创建账户</button>
                                </div>
                            </div>
                        </form>
                        </#if>

                        <#if personRegisterFlag == 2 >
                            <form class="" [formGroup]="personInfoForm" *ngIf="personInfoFlag" action="/perInfo">
                            <div class="text-left mx-auto g-width-40x--md">

                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">用户类型</label>
                                    <div class="col-sm-9">
                                        <select class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" formControlName="personType" style="height: 40px">
                                            <option value="">请选择...</option>
                                            <option value="{{1}}">个人</option>
                                            <option value="{{2}}">个体户</option>
                                        </select>
                                        <#--<div *ngIf="personInfoForm.controls['personType'].hasError('required') && personInfoForm.controls['personType'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 必须选择用户类型！</i>-->
                                        <#--</div>-->
                                    </div>
                                </div>
                                <input class=""  formControlName="merchantNo" hidden/>
                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">真实姓名</label>
                                    <div class="col-sm-9">
                                        <input class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" placeholder="请输入真实姓名"  formControlName="name"/>
                                        <#--<div *ngIf="personInfoForm.controls['name'].hasError('required') && personInfoForm.controls['name'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 姓名不能为空！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personInfoForm.controls['name'].hasError('validateNull') && personInfoForm.controls['name'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 姓名不能为空格！</i>-->
                                        <#--</div>-->
                                    </div>
                                </div>

                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">支付密码</label>
                                    <div class="col-sm-9">
                                        <input type="password" class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" placeholder="请输入真实姓名"  formControlName="payPassword"/>
                                        <#--<div *ngIf="personInfoForm.controls['payPassword'].hasError('required') && personInfoForm.controls['payPassword'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 支付密码不能为空！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personInfoForm.controls['payPassword'].hasError('validateNull') && personInfoForm.controls['payPassword'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 支付密码不能为空格！</i>-->
                                        <#--</div>-->
                                    </div>
                                </div>

                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">联系方式</label>
                                    <div class="col-sm-9">
                                        <input class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" placeholder="请输入真实姓名"  formControlName="mobile"/>
                                        <#--<div *ngIf="personInfoForm.controls['mobile'].hasError('required') && personInfoForm.controls['mobile'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 联系方式不能为空！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personInfoForm.controls['mobile'].hasError('validateNull') && personInfoForm.controls['mobile'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 联系方式不能为空格！</i>-->
                                        <#--</div>-->
                                    </div>
                                </div>

                                <div class="form-group row g-mb-25">
                                    <label class="col-sm-3 col-form-label g-mt-10 g-color-gray-dark-v4">身份证号</label>
                                    <div class="col-sm-9">
                                        <input class="form-control g-color-gray-dark-v5 g-bg-white g-bg-white--focus g-brd-gray-light-v3 g-brd-primary--hover rounded g-py-13 g-px-15" placeholder="请输入身份证号"  formControlName="idNo"/>
                                        <#--<div *ngIf="personInfoForm.controls['idNo'].hasError('required') && personInfoForm.controls['idNo'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 身份证号不能为空！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personInfoForm.controls['idNo'].hasError('validateNull') && personInfoForm.controls['idNo'].touched" class="text-warning bigger-110 orange">-->
                                            <#--<i class="ace-icon fa fa-exclamation-triangle"> 身份证号不能为空格！</i>-->
                                        <#--</div>-->
                                        <#--<div *ngIf="personInfoForm.controls['idNo'].hasError('minlength') || personInfoForm.controls['idNo'].hasError('maxlength') && personInfoForm.controls['idNo'].touched"  class="text-warning bigger-110 orange">-->
